local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library/main/Library", true))()

local window = library:AddWindow("MoonHub-PAID", {
    main_color = Color3.fromRGB(212, 175, 55), -- GUI color
    min_size = Vector2.new(470, 470), -- Size of the GUI
    can_resize = false,
})

local Main = window:AddTab("Main")
local folder = Main:AddFolder("Auto Brawl")


folder:AddSwitch("Auto Brawl Grind", function(state)
       autobrawlgrind = state
       if autobrawlgrind then
             task.spawn(function()
            while toggles.autoJoinToggleBrawl do
                game:GetService("ReplicatedStorage").rEvents.brawlEvent:FireServer("joinBrawl")
                task.wait(2)
            end
        end)
    end
end)

folder:AddSwitch("Auto Brawl GOD", function(state)
       autobrawlgod = state
       if autobrawlgod then
            task.spawn(function()
            while toggles.autoJoinToggleBrawlGOD do
                game:GetService("ReplicatedStorage").rEvents.brawlEvent:FireServer("joinBrawl")
                task.wait(0.5)
            end
        end)
    end
end)

local folder2 = Main:AddFolder("Anti Afk")

folder2:AddSwitch("Anti Afk", function(state)
     antiafk = state
     if antiafk then
         loadstring(game:HttpGet("https://raw.githubusercontent.com/evxncodes/mainroblox/main/anti-afk", true))()
end)


         
      
